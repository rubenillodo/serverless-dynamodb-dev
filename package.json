{
  "name": "serverless-dynamodb-dev",
  "version": "0.0.0-development",
  "description": "DynamoDB plugin for Serverless that creates and seeds tables in your development environment.",
  "author": "RubeÃÅn Illodo Brea <me@rubenillodo.com>",
  "license": "MIT",
  "repository": "git@github.com:rubenillodo/serverless-dynamodb-dev.git",
  "main": "dist/index.js",
  "scripts": {
    "dev": "yarn clean && nodemon",
    "test": "jest",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "tslint -p . -c tslint.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "aws-sdk": "^2.7.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.1.0",
    "@types/lodash": "^4.14.95",
    "jest": "^22.1.4",
    "nodemon": "^1.14.11",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.2.4",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-immutable": "^4.5.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2",
    "travis-deploy-once": "^4.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".d.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
